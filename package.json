{
  "name": "express-typescript",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/bin/index.js",
    "predev": "npm run build",
    "dev": "concurrently \"npx tsc -w\"  \"nodemon dist/bin/index.js\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "mongoose": "^8.0.3",
    "morgan": "~1.10.0",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.3.2"
  }
}
